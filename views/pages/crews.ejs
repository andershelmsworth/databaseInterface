<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ('../partials/head') %>
</head>
<body class="container">

<header>
    <%- include ('../partials/dashboardHeader') %>
</header>

<main>
    <div class="jumbotron">
        <h1>Crews</h1>
        <h3>View and adjust crews below.</h3>
		<table>
		<thead>
		<tr>
			<th>Crew ID</th>
			<th>Crew Name</th>
			<th></th>
		</thead>
		<tbody>
		<% myUsers.forEach(function(thing){ %>
        <tr>
			<td><%= thing.cid %></td>
			<td><input type="text" value="<%= thing.cname %>"></td>
			<td>
				<button type="button">Submit</button>
			</td>
			<td>
				<button type="button">Delete</button>
			</td>
		</tr>
      <% }) %>
	  </tbody>
	  </table>
    </div>
</main>

<footer>
    <%- include ('../partials/footer') %>
</footer>

</body>
</html>